{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=15",
    "npm": ">=8.5"
  },
  "scripts": {
    "dev": "vite",
    "build": "yarn build:ssr && yarn build:client",
    "build:client": "tsc && vite build --config vite.config.ts",
    "build:ssr": "tsc && vite build --config ssr.config.ts",
    "build:old": "tsc && vite build && npx tsx ./src/getUrlList.ts",
    "preview": "vite preview",
    "lint": "eslint . & stylelint src/**/*.{js,ts,tsx,mts} --fix",
    "format": "prettier --write .",
    "test": "jest ./",
    "link": "yarn link"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.0.0",
    "@reduxjs/toolkit": "^1.9.3",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "react-router": "^6.9.0",
    "react-router-dom": "^6.9.0",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "styled-components": "^5.3.9",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.1.1",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28",
    "jest-environment-jsdom": "^29.0.1",
    "lefthook": "^1.1.1",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^2.7.1",
    "stylelint": "^15.3.0",
    "stylelint-config-recommended": "^11.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2",
    "vite": "^3.0.7"
  },
  "license": "MIT"
}
